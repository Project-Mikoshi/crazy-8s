{
  "name": "crazy8s",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run tsc && node .next/src/server.js",
    "production": "npm run build && NODE_ENV=production node .next/src/server.js",
    "tsc": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./.next",
    "build": "next build && npm run tsc",
    "clean": "rm -rf .next",
    "lint": "eslint . --ext .jsx,.js,.ts,.tsx"
  },
  "dependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.3",
    "@types/express": "^4.17.14",
    "next": "13.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.8.4",
    "tscpaths": "^0.0.9",
    "@mui/material": "^5.10.3",
    "express": "^4.18.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "socket.io-client": "^4.5.3",
    "socket.io": "^4.5.3",
    "sass": "^1.56.1",
    "utf-8-validate": "^5.0.10",
    "bufferutil": "^4.0.7"
  }
}
